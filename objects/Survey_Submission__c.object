        <externalId>false</externalId>
        <formula>IF(NOT( ISBLANK(CSS__c)),IF( CSS__c &gt; 3 , 
	&apos;Excellent&apos; , 
	IF(CSS__c = 3 , 
	&apos;Good&apos; ,&apos;Poor&apos;))  , &apos;&apos; )</formula>
        <inlineHelpText>CSS Bucket is Poor if any score &lt;3, Good if any score = 3 with none &lt;3, Excellent if all scores &gt;3.</inlineHelpText>
        <label>CSS bucket</label>
