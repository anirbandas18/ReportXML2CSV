{!REQUIRESCRIPT(&quot;/soap/ajax/24.0/apex.js&quot;)}

var surveySubmission = new sforce.SObject(&quot;Survey_Submission__c&quot;);

if(&apos;{!Survey_Submission__c.Status__c}&apos; == &apos;Resent&apos;)
{
    alert(&quot;You can not resend survey if its already in Resent status&quot;);
}
else
{
    surveySubmission.id = &apos;{!Survey_Submission__c.Id}&apos;;
    surveySubmission.Status__c = &apos;ReSent&apos;;
    result = sforce.connection.update([surveySubmission]);
    if (result[0].getBoolean(&quot;success&quot;)) 
    {
       alert(&quot;Survey is resend successfully&quot;);
       window.location.href = &apos;/{!Survey_Submission__c.Id}&apos;;
    } 
    else 
    {
       alert(&quot;failed to resend&quot; + result[0]);
    }
}</url>
