	<fields>
        <fullName>Case_Reopen_Survey_Check__c</fullName>
        <description>Need to sent survey in following condition: 
1. Case re-open and closed. And survey is in Sent status. 
2. Case re-open and closed. and Survey is in submitted and responsed is register before 5 days.
</description>
        <externalId>false</externalId>
        <formula>
				IF( 
			OR( 
			AND( 
			NOT(ISBLANK(Case__r.Survey_Latest_Response_Date__c )), 
			NOT(ISBLANK(Case__r.ClosedDate )), 
			if((Case__r.ClosedDate - Case__r.Survey_Latest_Response_Date__c ) > 5, True, False) 
			), 
			if(ISBLANK(Case__r.Survey_Latest_Response_Date__c ), True, False), 
			if(Case__r.RecordType.DeveloperName = apos;Defaultapos;, True, False) 
			), 
			True, False)

)</formula>
        <formulaTreatBlanksAs>BlankAsBlanks</formulaTreatBlanksAs>
        <inlineHelpText></inlineHelpText>
        <label>Case Reopen Survey Check</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
        <unique>false</unique>
    </fields>
	
