    <validationRules>
        <fullName>Name_must_match_Region</fullName>
        <active>true</active>
        <description>This validation rule will require that the name of the Deal Approval Manager records are equal to the names of the Regions and cannot be changed to something different.</description>
        <errorConditionFormula>NOT(
OR(
Name= &quot;North America&quot;,
Name= &quot;India&quot;,
Name= &quot;EMEA&quot;,
Name= &quot;Japan&quot;,
Name= &quot;Asia Pacific&quot;)
)</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>The name of the Region must be North America, India, EMEA, Japan or Asia Pacific.</errorMessage>
    </validationRules>
