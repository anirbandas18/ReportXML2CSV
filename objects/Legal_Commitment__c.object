    <validationRules>
        <fullName>Required_Legal_Commitment_Status</fullName>
        <active>true</active>
        <description>Legal Commitment Status is required when either “Non-standard PQ/Exhibit Comments” or the “Special legal Review Required” is not blank</description>
        <errorConditionFormula>AND
( 
$Profile.Name &lt;&gt; &quot;5_1 - Data Steward&quot;,
$User.Is_Data_Steward__c = FALSE,
$User.LastName &lt;&gt; &apos;Migration&apos;,
NOT($Profile.Name = &apos;Integration&apos;),
OR( NOT(ISBLANK( Non_Standard_PQ_Exhibit_Comments__c )),
NOT(ISBLANK(  Special_Legal_Review_Required__c  ))) , 
ISPICKVAL(Legal_Commitment_Status__c, &quot;&quot;)
)</errorConditionFormula>
        <errorDisplayField>Legal_Commitment_Status__c</errorDisplayField>
        <errorMessage>Please select Legal Commitment status</errorMessage>
    </validationRules>
