    <validationRules>
        <fullName>CCR_Resubmit_and_State_combo_error</fullName>
        <active>false</active>
        <description>This validation will prevent a user from checking the Resubmit checkbox if the State of the CCR is not &apos;Review&apos;</description>
        <errorConditionFormula>AND(
$User.LastName &lt;&gt; &apos;Migration&apos;,
$User.LastName &lt;&gt; &apos;Integration&apos;,
$User.Is_Data_Steward__c = FALSE,
Resubmit__c = TRUE,
NOT(ISPICKVAL( State__c , &apos;Review&apos;))
)</errorConditionFormula>
        <errorDisplayField>Resubmit__c</errorDisplayField>
        <errorMessage>Only records in the state of &apos;Review&apos; can be resubmitted to CCMS.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Secure_Storage_equals_Not_Required</fullName>
        <active>false</active>
        <description>If Secure Storage = Not required then
1. Security Label must be one of the following
Confidential
Non Confidential</description>
        <errorConditionFormula>AND( $User.LastName &lt;&gt; &apos;Migration&apos;, 
$User.LastName &lt;&gt; &apos;Integration&apos;, 
$User.Is_Data_Steward__c = FALSE,
ISPICKVAL(Secure_Storage_Type__c, &apos;Not Required&apos;),  NOT(ISPICKVAL(Security_Label__c, &apos;Confidential&apos;)) ,  NOT(ISPICKVAL(Security_Label__c, &apos;Non-confidential&apos;)))</errorConditionFormula>
        <errorMessage>When Secure Storage = Not Required, then Security Label must be Confidential or Non Confidential</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Secure_Storage_equals_Secure_Chanber</fullName>
        <active>false</active>
        <description>1.  The Security Label must be one of the following
Confidential  Authorized group 
Confidential  Authorized users 
2.  The IT Ticket field must be filled out</description>
        <errorConditionFormula>IF( AND($User.LastName &lt;&gt; &apos;Migration&apos;, 
$User.LastName &lt;&gt; &apos;Integration&apos;, 
$User.Is_Data_Steward__c = FALSE, 
ISPICKVAL( Secure_Storage_Type__c , &apos;Secure Chamber&apos;)), OR(AND( NOT(ISPICKVAL(Security_Label__c, &apos;Confidential  authorized group&apos;)), NOT(ISPICKVAL(Security_Label__c, &apos;Confidential authorizedusers&apos;))), ISBLANK( IT_Ticket__c ) ), false)</errorConditionFormula>
        <errorMessage>When Secure Storage = Secure Chamber, then Security Label must be Confidential  Authorized group or Confidential  Authorized users. Additionally, the IT Ticket must be specified</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Secure_Storage_equals_Secure_Directory</fullName>
        <active>false</active>
        <description>If Secure Storage = Secure Directory then
1. The  Security Label must be one of the following
  Confidential  Delete date required 
  Confidential  Authorized group 
  Confidential  Authorized users 
2.  The IT Ticket field must be filled out</description>
        <errorConditionFormula>IF( AND($User.LastName &lt;&gt; &apos;Migration&apos;, 
$User.LastName &lt;&gt; &apos;Integration&apos;, 
$User.Is_Data_Steward__c = FALSE, 
ISPICKVAL( Secure_Storage_Type__c , &apos;Secure Directory&apos;)), OR(AND( NOT(ISPICKVAL(Security_Label__c, &apos;Confidential  authorized group&apos;)), NOT(ISPICKVAL(Security_Label__c, &apos;Confidential authorizedusers&apos;)), NOT(ISPICKVAL(Security_Label__c, &apos;Confidential  Delete date required&apos;))), ISBLANK( IT_Ticket__c ) ), false)</errorConditionFormula>
        <errorMessage>When Secure Storage = Secure Directory, then Security Label must be Confidential  Delete date required, Confidential  Authorized group or Confidential  Authorized users. Additionally, the IT Ticket must be specified</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Severity_Check_for_Request_Type</fullName>
        <active>false</active>
        <description>Severity should be either 1-Workaround or 2-Minor for enhancement request</description>
        <errorConditionFormula>AND( 
$User.LastName &lt;&gt; &apos;Migration&apos;, 
$User.LastName &lt;&gt; &apos;Integration&apos;, 
$User.Is_Data_Steward__c = FALSE, 
ISPICKVAL(RepSeverity__c,&apos;0 - No Workaround&apos;),
ISPICKVAL(Request_Type__c, &apos;Enhancement&apos;)
)</errorConditionFormula>
        <errorDisplayField>RepSeverity__c</errorDisplayField>
        <errorMessage>CCR Severity should be either 1-Workaround or 2-Minor for Enhancement Requests</errorMessage>
    </validationRules>
