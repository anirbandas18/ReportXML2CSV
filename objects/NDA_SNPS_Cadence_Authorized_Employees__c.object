IF(
ISPICKVAL(Related_Agreement__r.APTPS_IP_Product_Origin__c, &quot;Soft Core&quot;),
if(ISNULL(Last_Access_Date__c), Access_End_Date__c+365, Last_Access_Date__c+365),
if(ISNULL(Last_Access_Date__c), Access_End_Date__c+3*365, Last_Access_Date__c+3*365)
