        <externalId>false</externalId>
        <formula>IF(Never_Accessed__c = true , null,
/* INNER IF*/
IF( 
   AND( NOT(ISBLANK(Last_Access_Date__c )),
       ISPICKVAL(Related_Agreement__r.APTPS_IP_Product_Origin__c, &quot;Soft Core&quot;)), 
   Last_Access_Date__c + 365, /* END OF TRUE CLAUSE */
   
   IF(
       AND( NOT(ISBLANK(Last_Access_Date__c )),
            ISPICKVAL(Related_Agreement__r.APTPS_IP_Product_Origin__c, &quot;Hard Core&quot;)), 
       Last_Access_Date__c + 365 * 3, /* END OF TRUE CLAUSE */
       null
   )
) /* END OF INNER IF*/
)</formula>
