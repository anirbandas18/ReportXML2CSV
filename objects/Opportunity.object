        <externalId>false</externalId>
        <formula>IF( ISPICKVAL(Transaction_Type__c , &quot;IP&quot;),
    CASE(
        MAX(
            IF( TCV__c &gt; 3000000, 2, IF( TCV__c &gt; 500000, 1, 0)),
            IF( AND( TCV__c &gt; 500000, TCV__c &lt;= 3000000, Proposed_Discount__c &gt; 0.50), 2,
