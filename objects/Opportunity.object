    <fields>
        <fullName>Proposed_vs_Auth_Discount_Status__c</fullName>
        <externalId>false</externalId>
        <formula>/*

IF( New_SFDCOV__c  = &quot;New&quot;,IF( Order_Type_DMS__c &lt;&gt;&quot;1-SW&quot;,&quot;&quot;,IF( Proposed_Discount_DMS__c &lt;= min( Authorized_Deal_Discount_SFDC__c , Approved_Discount_Director__c , Approved_Discount_HQ__c , Approved_Discount_HQ_Admin__c , Approved_Discount_Region__c , Approved_Discount_VP__c ),&quot;Pass&quot;,&quot;Fail Disc&quot;)), Discount_Approval__c )

*/

IF( New_SFDCOV__c  = &quot;New&quot;,
    IF( Order_Type_DMS__c &lt;&gt;&quot;1-SW&quot;,
        &quot;&quot;, 
        CASE(
            IF( PropvsAuth__c &lt; 0, 1, 0)
            +
            IF(
                CASE(Approval_Level__c,
                    &quot;Director&quot;, Approved_Discount_Director__c,
                    &quot;Region&quot;, MIN(Approved_Discount_Director__c,
                                IF(ISBLANK(Approving_VP__c),999,Approved_Discount_VP__c),
                                Approved_Discount_Region__c
                            ),
                    MIN(Approved_Discount_Director__c,
                        IF(ISBLANK(Approving_VP__c),999,Approved_Discount_VP__c),
                        Approved_Discount_Region__c,
                        Approved_Discount_HQ_Admin__c,
                        Approved_Discount_HQ__c
                    )
                ) &lt;    ROUND(Authorized_Deal_Discount_SFDC__c,4)   ,
                2,      
                0
            ),
            0, &quot;Pass&quot;,
            1, &quot;Failing Discount&quot;,
            2, &quot;Incomplete Approval&quot;,
            &quot;Failing Discount, Incomplete Approval&quot;
        )
    ),
    Discount_Approval__c
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Proposed vs Auth Discount Status</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
