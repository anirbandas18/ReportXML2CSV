        <externalId>false</externalId>
        <formula>IF(NOT(ISPICKVAL( Transaction_Type__c , &quot;Serv&quot;)),&quot;&quot;,
   CASE(
/* Hosted Design Services */
        IF(INCLUDES( Service_Type__c , &quot;Hosted Design Services&quot;),
            IF(OR(   TCV__c  &gt; 1000000, AND( TCV__c &gt; 50000,  Proposed_Discount__c  &gt; 0.80)), 2,
                IF( OR( TCV__c &gt; 500000, AND( TCV__c &gt; 200000, Proposed_Discount__c  &gt; 0.65), AND( TCV__c &lt;= 200000, Proposed_Discount__c  &gt; 0.50)),1,0)
            ),
/* Pre-funded Startup */
            IF(INCLUDES( Service_Type__c , &quot;Pre-funded Startup&quot;),
                IF( TCV__c  &gt; 200000, 2,
                                        /* Ticket 00497493 2016.12.09 */
                    /* IF( AND( ASP_Ratio__c &lt; 0.25,  Service_Margin__c &lt;= 0.30),1,0) */
                                        IF( AND( DQM_Proposed_Core_SW__c  &lt; 0.25,  Service_Margin__c &lt;= 0.30),1,0)
                ),
/* Any other Services */
                IF(OR( TCV__c &gt; 1000000, AND(TCV__c &gt; 250000, Service_Margin__c &lt; 0.45)), 2,
                    IF( OR(TCV__c &gt; 250000, Service_Margin__c &lt; 0.45),1,0)
                )
            )
        ),
   0, &quot;Director&quot;,
   1, &quot;Region&quot;,
   2, &quot;HQ&quot;,
   &quot;Error&quot;
   )
)
