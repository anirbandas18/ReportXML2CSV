        ),
        AND(
            TCV__c&gt;=Deal_Approval_Region__r.Software_TCV_Threshold_1__c ,
            TCV__c&lt;Deal_Approval_Region__r.Software_TCV_Threshold_2__c ,
            Discount_Trend__c&gt;Deal_Approval_Region__r.Software_Discount_Trend_Threshold_2__c ,
            ASP_Ratio__c&gt; Deal_Approval_Region__r.Software_ASP_Ratio_Threshold_3__c ,
            Min_PS_ASP__c &gt;Deal_Approval_Region__r.Software_PS_ASP_Threshold_2__c 
        )
    ),1,
IF(
    OR( 
        AND( 
            TCV__c&lt; Deal_Approval_Region__r.Software_TCV_Threshold_1__c , 
            ASP_Ratio__c&gt;= Deal_Approval_Region__r.Software_ASP_Ratio_Threshold_1__c , 
            ASP_Ratio__c&lt;=Deal_Approval_Region__r.Software_ASP_Ratio_Threshold_2__c 
        ), 
        AND( 
            TCV__c&gt;= Deal_Approval_Region__r.Software_TCV_Threshold_1__c, 
            TCV__c&lt;Deal_Approval_Region__r.Software_TCV_Threshold_2__c, 
             Min_PS_ASP__c &gt;=Deal_Approval_Region__r.Software_PS_ASP_Threshold_1__c, 
            Min_PS_ASP__c&lt;=Deal_Approval_Region__r.Software_PS_ASP_Threshold_2__c 
        ), 
        AND( 
            TCV__c&gt;=Deal_Approval_Region__r.Software_TCV_Threshold_1__c, 
            TCV__c&lt;Deal_Approval_Region__r.Software_TCV_Threshold_2__c, 
            Discount_Trend__c&gt;= Deal_Approval_Region__r.Software_Discount_Trend_Threshold_1__c , 
            Discount_Trend__c&lt;=Deal_Approval_Region__r.Software_Discount_Trend_Threshold_2__c
        ), 
        AND( 
            TCV__c&gt;=Deal_Approval_Region__r.Software_TCV_Threshold_1__c, 
            TCV__c&lt;Deal_Approval_Region__r.Software_TCV_Threshold_2__c, 
            ASP_Ratio__c&gt;= Deal_Approval_Region__r.Software_ASP_Ratio_Threshold_2__c , 
            ASP_Ratio__c&lt;=Deal_Approval_Region__r.Software_ASP_Ratio_Threshold_3__c 
        ), 
        AND( 
            TCV__c&gt;=Deal_Approval_Region__r.Software_TCV_Threshold_2__c, 
            TCV__c&lt;Deal_Approval_Region__r.Software_TCV_Threshold_3__c, 
            Discount_Trend__c&gt;= Deal_Approval_Region__r.Software_Discount_Trend_Threshold_1__c, 
            ASP_Ratio__c&gt;= Deal_Approval_Region__r.Software_ASP_Ratio_Threshold_2__c , 
            Min_PS_ASP__c &gt;=Deal_Approval_Region__r.Software_PS_ASP_Threshold_1__c 
        ) 
    ),3,
IF(
    OR( 
        AND( 
            TCV__c&lt; Deal_Approval_Region__r.Software_TCV_Threshold_1__c , 
            ASP_Ratio__c&lt; Deal_Approval_Region__r.Software_ASP_Ratio_Threshold_1__c 
