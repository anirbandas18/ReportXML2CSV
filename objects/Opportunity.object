    <validationRules>
        <fullName>AX_Name_Required</fullName>
        <active>true</active>
        <description>If probability is greater or equal to 30% AX Name is required.</description>
        <errorConditionFormula>AND($Setup.Data_Migration__c.Data_Migration_Bypass__c == false, 
$Profile.Name != &apos;5_1 - Data Steward&apos;, 
NOT(CONTAINS($User.LastName , &apos;Migration&apos;)), 
NOT(ISPICKVAL(StageName, &quot;10%&quot;)), 
NOT(Apex_context__c=&apos;TRUE&apos;), 
$User.Opty_Bypass__c &lt;&gt; true, 
RecordType.DeveloperName = &apos;IP&apos;, 
Expected_Close_Quarter_1__r.Quarter_Start_Date__c &gt;= DATEVALUE(&quot;2013-12-29&quot;), ISBLANK( AX_Name__c ))</errorConditionFormula>
        <errorDisplayField>AX_Name__c</errorDisplayField>
        <errorMessage>If probability is greater or equal to 30% AX Name is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Billing_Plan_Even_Quarterly</fullName>
        <active>false</active>
        <description>Where M/3 = N,  if N = 1, the user should select “100% upfront” for Payment Plan.</description>
        <errorConditionFormula>AND(ISPICKVAL(Billing_Plan__c,&apos;Even Quarterly&apos;), (Term__c / 3 = 1),  NOT(ISCHANGED(Validation_Override__c)),
 $User.Opty_Bypass__c  &lt;&gt; true
)</errorConditionFormula>
        <errorDisplayField>Billing_Plan__c</errorDisplayField>
        <errorMessage>Please select “100% upfront” for Billing Plan.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Billing_Plan_Semi_Annual</fullName>
        <active>false</active>
        <description>Where M/6 = N,  if N = 1, the user should select “100% upfront” for Payment Plan.</description>
        <errorConditionFormula>AND(ISPICKVAL(Billing_Plan__c,&apos;Even Semi-Annually&apos;), (Term__c / 6 = 1), NOT(ISCHANGED(Validation_Override__c)),
 $User.Opty_Bypass__c  &lt;&gt; true)</errorConditionFormula>
        <errorDisplayField>Billing_Plan__c</errorDisplayField>
        <errorMessage>Please select “100% upfront” for Billing Plan.</errorMessage>
    </validationRules>
