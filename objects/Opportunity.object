    <fields>
        <fullName>Fast_Track_Eligible__c</fullName>
        <externalId>false</externalId>
        <formula>IF( 
    AND( 
        NOT(ISBLANK( Approval_Hierarchy__c )), 
        
        ISPICKVAL(Terms_and_Conditions__c, &apos;STD&apos;), 
        
        OR( 
            ISPICKVAL(Transaction_Type__c, &apos;99-YR/Other&apos;), 
            ISPICKVAL(Transaction_Type__c, &apos;TBL&apos;), 
            ISPICKVAL(Transaction_Type__c, &apos;IP - Tensilica&apos;) 
        ), 
        
        IF( 
            OR( 
                ISPICKVAL(Transaction_Type__c, &apos;99-YR/Other&apos;), 
                AND( 
                    ISPICKVAL(Transaction_Type__c, &apos;TBL&apos;), 
                    ASP_Version__c = &apos;ASP&apos;                                                                
                ) 
            ),             
            
            AND( 
                ASP_Ratio_SFDC__c &gt;= 0.8, 
                Discount_Trend_SFDC__c &gt;= 0 
            ),             
            
            IF( 
                AND( 
                    ISPICKVAL(Transaction_Type__c, &apos;TBL&apos;), 
                    ASP_Version__c = &apos;DQM&apos;
                ), 
                AND( 
                    DQM_Proposed_Core_SW__c &gt;= 0.8, 
                    DQM_Trend_Core_SW__c &gt;= 0 
                ), 
                NOT(
                    AND(
                        ISPICKVAL(Transaction_Type__c, &apos;TBL&apos;),
                        ASP_Version__c &lt;&gt; &apos;ASP&apos;,
                        ASP_Version__c &lt;&gt; &apos;DQM&apos;
                    )
                )
            ) 
        ), 
        
        IF(
            ISPICKVAL(Transaction_Type__c, &apos;99-YR/Other&apos;), 
            AND( 
                NOT(ISBLANK(Primary_Quote_name__c)), 
                TCV__c &lt;= 1000000 
            ), 
            true 
        ),
        
        IF( ISPICKVAL(Transaction_Type__c, &apos;TBL&apos;), 
            AND( 
                TCV__c &lt;= 1000000, 
                SQW_Uploaded_to_DMS__c = &apos;Yes&apos;, 
                ISPICKVAL(Payment_Terms__c, &apos;NET 30&apos;), 
                OR( 
                    ISPICKVAL(Billing_Plan__c, &apos;100% Upfront&apos;), 
                    ISPICKVAL(Billing_Plan__c, &apos;Even Quarterly&apos;), 
                    ISPICKVAL(Billing_Plan__c, &apos;Even Semi-Annually&apos;), 
                    ISPICKVAL(Billing_Plan__c, &apos;Even Annually&apos;) 
                ) 
            ), 
            true 
        ), 
        
        IF( ISPICKVAL(Transaction_Type__c, &apos;IP - Tensilica&apos;), 
            AND( 
                TCV__c &lt;= 150000, 
                ProposedDiscount__c &lt; 20.0, 
                ISPICKVAL(Payment_Terms__c, &apos;NET 30&apos;), 
                ISPICKVAL(Billing_Plan__c, &apos;100% Upfront&apos;) 
            ), 
            true 
        )                   
    ), 
    true, 
    false 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
