        <fullName>Board_Required_When_Xtreme</fullName>
        <active>true</active>
        <description>If Hardware Platform = Xtreme, then Boards is required</description>
        <errorConditionFormula>AND(
$Setup.Data_Migration__c.Data_Migration_Bypass__c == false, 
NOT(Apex_context__c=&apos;TRUE&apos;), 
$Profile.Name != &apos;5_1 - Data Steward&apos;,
NOT(ISCHANGED(Validation_Override__c)),
ISPICKVAL( HW_Platform__c , &apos;Xtreme&apos;), ISBLANK(Boards__c),
 $User.Opty_Bypass__c  &lt;&gt; true )</errorConditionFormula>
        <errorDisplayField>Boards__c</errorDisplayField>
        <errorMessage>If Hardware Platform = Xtreme, then Boards is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BoardsRequired</fullName>
        <active>false</active>
        <errorConditionFormula>IF(NOT(ISPICKVAL(HW_Platform__c, &apos;Rapid Prototyping&apos;)),
AND(
$Setup.Data_Migration__c.Data_Migration_Bypass__c  == false,
 $User.Opty_Bypass__c  &lt;&gt; true,
NOT(ISCHANGED(Validation_Override__c)),
OR(ISPICKVAL(Transaction_Type__c, &apos;Hardware - 99 Year&apos;), ISPICKVAL(Transaction_Type__c, &apos;Hardware - Lease&apos;)),
OR(ISBLANK( Boards__c ))), False)</errorConditionFormula>
