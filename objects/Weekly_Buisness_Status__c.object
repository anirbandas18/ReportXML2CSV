    <fields>
        <fullName>Week_end__c</fullName>
        <externalId>false</externalId>
        <formula>IF( NOT( ISNULL(CreatedDate)) ,
CASE(
MOD(DATEVALUE(CreatedDate) - DATE(1900, 1, 7), 7),
1, DATEVALUE(CreatedDate)+ 4,
2, DATEVALUE(CreatedDate)+ 3,
3, DATEVALUE(CreatedDate)+ 2,
4, DATEVALUE(CreatedDate)+ 1,
5, DATEVALUE(CreatedDate)+ 0,
6, DATEVALUE(CreatedDate)+ 6,
0, DATEVALUE(CreatedDate)+5,null),
CASE(
MOD(TODAY() - DATE(1900, 1, 7), 7),
1, today() + 4,
2, today() + 3,
3, today() + 2,
4, today() + 1,
5, today() + 0,
6, today() + 6,
0, today() +5,null)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Week end date is usually the Friday of the current week. The system auto calculates the date to the next friday</inlineHelpText>
        <label>Week end date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
