    <webLinks>
        <fullName>Mark_Task_Completed</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Mark Task Completed</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!requireScript(&quot;/soap/ajax/24.0/connection.js&quot;)}
var taskRecords = sforce.connection.query(&quot;SELECT Id, Status from Task Where Id = &apos;{!Task.Id}&apos;&quot;);

var task = taskRecords.getArray(&apos;records&apos;)[0];

if(task.Status !=&quot;Completed&quot;){
 task.Status = &quot;Completed&quot;;
 result = sforce.connection.update([task]);

 if (result[0].getBoolean(&quot;success&quot;))
 {
  window.parent.location.href = &quot;/{!Task.Id}&quot;;
 }
 else
 {
  alert(&quot;Failed to update task: &quot; + result[0].errors.message);
 }
}</url>
