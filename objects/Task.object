    <validationRules>
        <fullName>ArticlePublisher_on_OwnerChange</fullName>
        <active>true</active>
        <description>Task should not be transferred to a User, who don&apos;t have Article_Publisher Permissions.</description>
        <errorConditionFormula>AND( $Profile.Name &lt;&gt; &quot;5_1 - Data Steward&quot;, 
$User.LastName &lt;&gt; &apos;Migration&apos;, 
NOT($Profile.Name = &apos;Integration&apos;), 
AND(OR( ISPICKVAL(  Activity_Type__c , &apos;Article Creation Request&apos;) , ISPICKVAL(  Activity_Type__c , &apos;Article Maintenance Request&apos;),ISPICKVAL(  Activity_Type__c , &apos;Expired Article Review&apos;) , ISPICKVAL(  Activity_Type__c , &apos;Article Publish Request&apos;)), 
NOT( Owner:User.Article_Publisher__c ))
, if(WhatId != null , NOT(BEGINS(WhatId , &apos;a4H&apos;)) , true)
, if(WhatId != null , NOT(BEGINS(WhatId , &apos;a4K&apos;)) , true)
)</errorConditionFormula>
        <errorDisplayField>OwnerId</errorDisplayField>
        <errorMessage>You can not transfer the Task to this user. Because this user do not have Article Publisher permission.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Comment_Section_mandatory_check</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT($Profile.Name = &quot;5_1 - Data Steward&quot;), 
	NOT($User.LastName = &apos;Migration&apos;), 
	NOT($User.LastName = &apos;Code Migration&apos;),  
	NOT($Profile.Name = &apos;Integration&apos;),
	OR(RecordType.DeveloperName = &apos;ACR&apos;) ,
	ISBLANK(Description))</errorConditionFormula>
        <errorDisplayField>Description</errorDisplayField>
        <errorMessage>Comments section should not be blank.</errorMessage>
    </validationRules>
