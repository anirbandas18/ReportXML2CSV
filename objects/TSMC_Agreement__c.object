if(records.length &gt; maxsize.getArray(&quot;records&quot;)[0].Value__c)
{
   alert(&apos;You can select maximum &apos;+maxsize.getArray(&quot;records&quot;)[0].Value__c+&apos; records for bulk completion&apos;);
}
else
{
if(records.length &lt; 1)
{
   alert(&apos;Invalid Operation. Please select 1 or more row(s).&apos;);
}
else
{
var result = sforce.connection.query(&quot;SELECT Id, Status__c FROM TSMC_Agreement__c where Status__c=&apos;Fully Signed&apos; and Id in (&quot;+commarecords+&quot;)&quot;); 

if(result.getArray(&quot;records&quot;).length != records.length)
{
  alert(&quot;Please select fully signed requests only for bulk completion&quot;);
}
else
{
    var newrecords = [];
    for (var n=0; n&lt;records.length; n++) { 
    var c = new sforce.SObject(&quot;TSMC_Agreement__c&quot;); 
    c.id = records[n]; 
    c.Status__c = &quot;Completed&quot;; 
    newrecords.push(c); 
    } 
    result = sforce.connection.update(newrecords);
    alert(&quot;All TSMC Request(s) are completed successfully.&quot;);
}

}

}
}
else
{
    alert(&quot;You are not authorised to do this operation&quot;);
}
window.location.replace(&apos;/aEl&apos;);</url>
