var user = sforce.connection.getUserInfo();
var group = sforce.connection.query(&quot;SELECT Id FROM GroupMember WHERE UserOrGroupId = &apos;&quot;+user.userId+&quot;&apos; AND Group.Type = &apos;Queue&apos; AND Group.Name = &apos;TSMC Admin&apos;&quot;);
    
if(group.getArray(&quot;records&quot;).length &gt; 0)
{

