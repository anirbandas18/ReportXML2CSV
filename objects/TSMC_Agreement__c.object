    <webLinks>
        <fullName>Approve</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Approve</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/36.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/36.0/apex.js&quot;)}

var records = {!GETRECORDIDS($ObjectType.TSMC_Agreement__c)};
for (var i = 0; i &lt; records.length; i++) { 
    records[i] = &quot;&apos;&quot;+records[i]+&quot;&apos;&quot;;
}

var user = sforce.connection.getUserInfo();
var group = sforce.connection.query(&quot;SELECT Id FROM GroupMember WHERE UserOrGroupId = &apos;&quot;+user.userId+&quot;&apos; AND Group.Type = &apos;Queue&apos; AND Group.Name = &apos;TSMC Admin&apos;&quot;);
    
if(group.getArray(&quot;records&quot;).length &gt; 0)
{

var maxsize = sforce.connection.query(&quot;select Value__c from TSMC_Helper__c where name = &apos;maxapprove&apos;&quot;);
if(records.length &gt; maxsize.getArray(&quot;records&quot;)[0].Value__c)
{
   alert(&apos;You can select maximum &apos;+maxsize.getArray(&quot;records&quot;)[0].Value__c+&apos; records for bulk approval&apos;);
}
else
{
if(records.length &lt; 1)
{
   alert(&apos;Invalid Operation. Please select 1 or more row(s).&apos;);
}
else
{
   var result = sforce.connection.query(&quot;SELECT Id, Status__c, Comments__c FROM TSMC_Agreement__c where Status__c = &apos;Pending For Approval&apos; and Id in (&quot;+records+&quot;)&quot;); 

   if(result.getArray(&quot;records&quot;).length != records.length)
   {  
     alert(&quot;Please select pending approval request only for bulk approval&quot;);
   }
   else
   {
     var myMap = new Map();
     var records1 = result.getArray(&quot;records&quot;);
     for (var i=0; i&lt;records1.length; i++) {
       myMap.put(records1[i].Id, records1[i].Comments__c);   
     }  

        var result = sforce.connection.query(&quot;SELECT Id, Status, TargetObjectId FROM  ProcessInstance where Status=&apos;Pending&apos; and TargetObjectId in (&quot;+records+&quot;)&quot;, {
       onSuccess : success,
       onFailure : failure
       }); 

       function success(result) 
       {
          var records = result.getArray(&quot;records&quot;);
          for (var i=0; i&lt;records.length; i++) {
             var record = records[i];
             findid(record.Id, record.TargetObjectId);   
          }
          alert(&quot;All TSMC Request(s) are approved successfully.&quot;);
       }

       function failure(error) {
          alert(&quot;An error has occurred &quot; + error);
       }

       function findid(record, TargetObjectId)
       {
          var result = sforce.connection.query(&quot;SELECT Id, ProcessInstanceId FROM ProcessInstanceWorkitem WHERE ProcessInstanceId = &apos;&quot;+record+&quot;&apos;&quot;);
          var records = result.getArray(&quot;records&quot;);
          for (var i=0; i&lt;records.length; i++) {
             var record = records[i];
             var request = new sforce.ProcessWorkitemRequest();
             request.action = &quot;Approve&quot;;
             request.workitemId = record.Id;
             if(myMap.get(TargetObjectId) != undefined)
             {
                request.comments = myMap.get(TargetObjectId);
             }
             var processRes = sforce.connection.process([request]);
             if(!processRes[0].getBoolean(&apos;success&apos;)){
                 alert(&quot;The first process request failed and it should not have&quot;+processRes[0].errors);
             }
         }
       }

   }
}
}
}
else
{
   alert(&quot;You are not authorised to do this operation&quot;);
}

    setTimeout(function(){window.location.replace(&apos;/aEl&apos;);}, 3000);</url>
    </webLinks>
