        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/connection.js&quot;)}

var userId = sforce.connection.getUserInfo().userId;

var userAccessQry = &quot;SELECT  RecordId  ,HasEditAccess &quot; +
    &quot;FROM UserRecordAccess WHERE UserId = &apos;&quot; + userId + &quot;&apos; AND RecordId = &quot; +
    &quot;&apos;{!ER_Agreement__c.Id}&apos;&quot;;
var userAccessResult = sforce.connection.query(userAccessQry);
var UserAccessRecords = userAccessResult.getArray(&quot;records&quot;);


