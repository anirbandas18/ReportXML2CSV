        <relationshipLabel>Related Articles (Article2)</relationshipLabel>
        <relationshipName>Related_Articles2</relationshipName>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Related_Article_Id__c</fullName>
        <description>Generates a linkable Related Article Id for use in related list</description>
        <externalId>false</externalId>
        <formula>HYPERLINK(&quot;/&quot;&amp;Id, Article2__r.Name)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Article Id</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Related_Article_Last_Modified_Date__c</fullName>
        <externalId>false</externalId>
        <formula>Article2__r.LastModifiedDate</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Last Modified</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>Related_Article_Title__c</fullName>
        <description>Creates a link to the Related Article&apos;s Title.</description>
        <externalId>false</externalId>
        <formula>HYPERLINK(&quot;/&quot;&amp;Id, Article2__r.Title__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Title</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Related_Article__c</fullName>
