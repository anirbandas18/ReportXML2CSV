    <fields>
        <fullName>Article1__c</fullName>
        <deleteConstraint>Restrict</deleteConstraint>
        <externalId>false</externalId>
        <label>Parent Article</label>
        <referenceTo>Article__c</referenceTo>
        <relationshipLabel>Related Articles</relationshipLabel>
        <relationshipName>Related_Articles</relationshipName>
        <required>true</required>
        <trackTrending>false</trackTrending>
