    <validationRules>
        <fullName>ACR_Close_check</fullName>
        <active>true</active>
        <description>To check the conditions before closing the ACR Task</description>
        <errorConditionFormula>AND(
        ISPICKVAL(Activity_Type__c, &quot;Article Creation Request&quot;),
        ISPICKVAL( Status__c , &quot;Completed&quot;),
        OR(
           ISBLANK(Article__c), 
           AND(
               upper(Submitter__c) != upper(Article__r.Submitter__r.Email),
               upper(Submitter__c) != upper(Article__r.Submitter__r.Login_Name__c),
               upper(Submitter__c) != upper(Article__r.Submitter__r.FirstName+&apos; &apos;+Article__r.Submitter__r.LastName)  
               )
           )   
       )</errorConditionFormula>
        <errorMessage>No Article is linked to the ACR OR Article&apos;s Author &amp; Queued Task&apos;s Submitter are not the same.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AMR_Close_check</fullName>
        <active>true</active>
        <description>To check the conditions before closing the AMR Task</description>
        <errorConditionFormula>AND(
     ISPICKVAL(Activity_Type__c, &quot;Article Maintenance Request&quot;),
     ISPICKVAL( Status__c , &quot;Completed&quot;),
     ISBLANK(Article__c)
    )</errorConditionFormula>
        <errorMessage>No Article is linked to the AMR task.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Comments_should_not_be_null_for_Article</fullName>
        <active>true</active>
        <description>Comments should not be null for Article Related Queued Tasks</description>
        <errorConditionFormula>AND($Profile.Name &lt;&gt; &quot;5_1 - Data Steward&quot;, 
$User.LastName &lt;&gt; &apos;Migration&apos;, 
$User.LastName &lt;&gt; &apos;Code Migration&apos;, 
$Profile.Name &lt;&gt; &apos;Integration&apos;, 
(TEXT(Activity_Type__c) = &apos;Expired Article Review&apos; || TEXT(Activity_Type__c) = &apos;Article Creation Request&apos; || 
TEXT(Activity_Type__c) = &apos;Article Publish Request&apos; || 
TEXT(Activity_Type__c) = &apos;Article Maintenance Request&apos; || TEXT(Activity_Type__c) = &apos;Knowledge Maintenance Request&apos;), 
Comments__c = &apos;&apos; 
)</errorConditionFormula>
        <errorDisplayField>Comments__c</errorDisplayField>
        <errorMessage>Please enter &apos;Comments&apos;, Comments should not be blank for Article Related Tasks</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Select_Only_one_related_To</fullName>
        <active>true</active>
        <description>prevent users from selecting more than one of the following fields: &quot;Case&quot;, &quot;Article&quot;, &quot;CCR&quot;, &quot;Account&quot;, &quot;Contact&quot; or &quot;User&quot;. &quot;Product&quot; may be selected without &quot;Case&quot;, &quot;Article&quot;, or &quot;CCR&quot;.

Old Error Message: User is allowed to select one &quot;Case&quot;, &quot;Artic</description>
        <errorConditionFormula>IF(AND(ISBLANK(Product_Class__c), OR( 
AND(ISBLANK(Case__c) , ISBLANK(Article__c) , ISBLANK(CCR__c),ISBLANK(Account__c), ISBLANK(User__c), ISBLANK(Contact__c)) 
, AND(! ISBLANK(Case__c) , ! ISBLANK(Article__c)) 
, AND( ! ISBLANK(Case__c) , ! ISBLANK(CCR__c)) 
, AND( ! ISBLANK(Article__c) , ! ISBLANK(CCR__c)) 
, AND( ! ISBLANK(Case__c) , ! ISBLANK(Account__c)) 
, AND( ! ISBLANK(Account__c) , ! ISBLANK(CCR__c)) 
, AND( ! ISBLANK(Account__c) , ! ISBLANK(Article__c)) 
, AND( ! ISBLANK(Case__c) , ! ISBLANK(User__c)) 
, AND( ! ISBLANK(User__c) , ! ISBLANK(CCR__c)) 
, AND( ! ISBLANK(User__c) , ! ISBLANK(Account__c)) 
, AND( ! ISBLANK(User__c) , ! ISBLANK(Article__c)))), true, false)</errorConditionFormula>
        <errorMessage>&quot;User must specify Case, Product or Article&quot;</errorMessage>
    </validationRules>
