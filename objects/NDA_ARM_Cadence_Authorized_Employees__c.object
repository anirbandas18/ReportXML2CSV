    <fields>
        <fullName>Ref_ARM_Restriction_Applied__c</fullName>
        <externalId>false</externalId>
        <formula>&quot;Employee is restricted from working in specific areas of Cadence’s business due to employee’s access of ARM data. You may follow the Document Link to view details related to this restriction.&quot;</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Ref_ARM_Restriction_Applied</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Ref_ARM_Restriction_End_Date__c</fullName>
        <externalId>false</externalId>
        <formula>if(Never_Accessed__c,NULL,(if(ISNULL(Last_Access_Date__c), Access_End_Date__c+365, Last_Access_Date__c+365)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Ref_ARM_Restriction_End_Date</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Ref_ARM_Restriction_ProjectID__c</fullName>
        <externalId>false</externalId>
        <label>Ref_ARM_Restriction_ProjectID</label>
        <length>10</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Ref_ARM_Restriction_ProjectName__c</fullName>
        <externalId>false</externalId>
