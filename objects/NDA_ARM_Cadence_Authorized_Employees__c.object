        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Refrigeration_End_Date__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Never_Accessed__c = true  , null ,  
IF( NOT(ISBLANK(Last_Access_Date__c )),  Last_Access_Date__c + 365 , 
IF( NOT(ISBLANK(Access_End_Date__c )),  Access_End_Date__c + 365 ,null)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Refrigeration End Date</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Related_Agreement__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <externalId>false</externalId>
        <label>Related Agreement</label>
        <referenceTo>Apttus__APTS_Agreement__c</referenceTo>
        <relationshipLabel>NDA ARM Cadence Authorized Employees</relationshipLabel>
        <relationshipName>NDA_ARM_Cadence_Authorized_Employees</relationshipName>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Status__c</fullName>
        <description>Show the Status as Active if Never Accessed is false else show Inactive</description>
        <externalId>false</externalId>
        <formula>IF( Never_Accessed__c , &quot;Inactive&quot;, &quot;Active&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Employee Status</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
