        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Ref_ARM_Restriction_Reason__c</fullName>
        <externalId>false</externalId>
        <formula>if (Never_Accessed__c,&quot;Never_Accessed&quot;, NULL)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Ref_ARM_Restriction_Reason</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Ref_ARM_Restriction_Start_Date__c</fullName>
        <externalId>false</externalId>
        <formula>if (Never_Accessed__c, NULL, if(ISNULL(Related_Agreement__r.Apttus__Activated_Date__c),DATEVALUE(Related_Agreement__r.LastModifiedDate),Related_Agreement__r.Apttus__Activated_Date__c))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Ref_ARM_Restriction_Start_Date</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Ref_ARM_Restriction_Status__c</fullName>
        <externalId>false</externalId>
        <formula>if(Never_Accessed__c,true,false)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Ref_ARM_Restriction_Status</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
