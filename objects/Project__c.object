    <validationRules>
        <fullName>Comma_separate_External_emailId</fullName>
        <active>true</active>
        <description>For external alias, we could not add emails as csv</description>
        <errorConditionFormula>if( CONTAINS(Project_External_Email_Alias__c, &apos;,&apos; ), true, false )</errorConditionFormula>
        <errorDisplayField>Project_External_Email_Alias__c</errorDisplayField>
        <errorMessage>Please provide valid email addresses separated by semicolon, e.g. email@domain.com;email2@domain2.com</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Comma_separate_InternaemailId_no_allowed</fullName>
        <active>true</active>
        <description>For internal alias, we could not add emails as csv</description>
        <errorConditionFormula>if( CONTAINS(Project_internal_email_alias__c, &apos;,&apos; ), true, false )</errorConditionFormula>
        <errorDisplayField>Project_internal_email_alias__c</errorDisplayField>
        <errorMessage>Please provide valid email addresses separated by semicolon, e.g. email@domain.com;email2@domain2.com</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Project_Name_must_be_Unique</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
	$Profile.Name &lt;&gt; &quot;5_1 - Data Steward&quot;,
	$User.LastName &lt;&gt; &apos;Migration&apos;,
        $User.LastName &lt;&gt; &apos;Code Migration&apos;,
	NOT($Profile.Name = &apos;Integration&apos;),
	AND(
		VLOOKUP($ObjectType.Project__c.Fields.Name,  
		$ObjectType.Project__c.Fields.Name , Name)  =  Name     
	),
	OR(
		ISNEW(),
		ISCHANGED(Name)
	)  
)</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>Project Name must be unique.</errorMessage>
    </validationRules>
