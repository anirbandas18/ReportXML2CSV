        <externalId>false</externalId>
        <label>Data Migration Bypass</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>gss_del_trigger_bypass__c</fullName>
        <externalId>false</externalId>
        <label>gss_del_trigger_bypass</label>
        <length>50</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>gss_ins_trigger_bypass__c</fullName>
