        <externalId>false</externalId>
        <inlineHelpText>bypass code is 2655 
Dont put this value unless you want to stop the insert trigger</inlineHelpText>
        <label>gss_ins_trigger_bypass</label>
        <length>50</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
