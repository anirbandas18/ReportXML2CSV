        <externalId>false</externalId>
        <formula>If( Not(ISPICKVAL( Email_Type__c , &quot;&quot;)), 
IMAGE( 
IF( ISPICKVAL( Email_Type__c , &quot;Severity Email&quot;), &quot;/resource/red_dot&quot;,
IF( ISPICKVAL( Email_Type__c , &quot;COS Comment&quot;), &quot;/resource/grey_dot&quot;, 
IF( ISPICKVAL( Email_Type__c , &quot;Write Case Note&quot;), &quot;/resource/blue_dot&quot;, 
IF( ISPICKVAL( Email_Type__c , &quot;Owner Email&quot;), &quot;/resource/yellow_dot&quot;,
IF( ISPICKVAL( Email_Type__c , &quot;Contact Notification&quot;), &quot;/resource/brown_dot&quot;,
IF( ISPICKVAL( Email_Type__c , &quot;Answer Customer Email&quot;), &quot;/resource/black_dot&quot;,
IF( ISPICKVAL( Email_Type__c , &quot;Jira comment&quot;), &quot;/resource/purple_dot&quot;,
IF( ISPICKVAL( Email_Type__c , &quot;Case Auto Closed&quot;), &quot;/resource/grey_dot&quot;, 
IF( ISPICKVAL( Email_Type__c , &quot;Customer Email&quot;), &quot;/resource/green_dot&quot;,&apos;&apos;
))))))))), &quot;Error&quot;),&apos;&apos;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
