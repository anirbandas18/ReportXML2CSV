    <validationRules>
        <fullName>Cannot_select_closed_case</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Case__r.Status , &apos;Closed&apos;) || ISPICKVAL(Case__r.Status , &apos;Completed&apos;)</errorConditionFormula>
        <errorDisplayField>Case__c</errorDisplayField>
        <errorMessage>Cannot select a Case which is Closed or Completed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contact_assoc_Product_must_Acc_ParAcc</fullName>
        <active>true</active>
        <description>Prevents a contact from being associated to a Product if (Account or Parent Account) blank</description>
        <errorConditionFormula>AND (Contact__c != null , Product__c != null,Account__c == null, Parent_Account__c == null)</errorConditionFormula>
        <errorMessage>IF Contacts have a product Attribute specified, they must also have either a parent account or Account Specified as well.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contact_must_not_be_follow_Chatter</fullName>
        <active>true</active>
        <description>NOT ALLOWED :-
Case Subscription record that is associated to a contact from checking the &quot;Follow on Chatter&quot; checkbox.</description>
        <errorConditionFormula>AND(Contact__c != null,  User__c == null ,Follow_on_Chatter__c)</errorConditionFormula>
        <errorDisplayField>Follow_on_Chatter__c</errorDisplayField>
        <errorMessage>A record that is associated to a Contact must not allowed to Follow on Chatter.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Not_Populate_Case_If_CCEmail_Blank</fullName>
        <active>false</active>
        <description>Prevents All Customer Communication from being checked unless there is a Case and CC Emails populated.</description>
        <errorConditionFormula>AND( Case__c != null,  All_Customer_Communication__c=TRUE , ISBLANK(Case_CC_Emails__c ) )</errorConditionFormula>
        <errorDisplayField>Case__c</errorDisplayField>
        <errorMessage>Must provide CC emails if Case is populated and All Customer Communication is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prod_Feat_Lvl_1_Req_for_Lvl_2</fullName>
        <active>true</active>
        <description>This prevents the selection of a Product Feature Level 2 if a Product Feature Level 1 has not been specified.</description>
        <errorConditionFormula>AND (
ISBLANK( Product_Feature_Level_1__c ),
NOT(ISBLANK( Product_Feature_Level_2__c ) )
)</errorConditionFormula>
        <errorMessage>Product Feature Level 2 cannot be selected if a Product Feature Level 1 has not been specified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prod_Feat_Req_for_Lvl_1</fullName>
        <active>true</active>
        <description>This prevents the selection of a Product Feature Level 1 if a Product Feature has not been specified.</description>
        <errorConditionFormula>AND (
ISBLANK(  Product_Feature__c  ),
NOT(ISBLANK( Product_Feature_Level_1__c ) )
)</errorConditionFormula>
        <errorMessage>Product Feature Level 1 cannot be selected if a Product Feature has not been specified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Product_Required_for_Product_Feature</fullName>
        <active>true</active>
        <description>This prevents the selection of a Product Feature if a Product has not been specified.</description>
        <errorConditionFormula>AND (
ISBLANK(  Product__c ),
NOT(ISBLANK(  Product_Feature__c  ) )
)</errorConditionFormula>
        <errorDisplayField>Product_Feature__c</errorDisplayField>
        <errorMessage>Product Feature cannot be selected if a Product Attribute has not been specified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ValidateFieldsOn_CaseAndCCEmail_Populate</fullName>
        <active>true</active>
        <description>Prevents any other fields from being populated if Case and Case CC Emails are populated.</description>
        <errorConditionFormula>AND( 
	Case__c != null,  Case_CC_Emails__c != null,  
	AND(
		ISCHANGED(Case__c ) , 
		ISCHANGED(Case_CC_Emails__c ) ,
		OR(
				ISCHANGED(Account__c) , 
				ISCHANGED(All_Customer_Communication__c) , 
				ISCHANGED(Contact__c) , 
				ISCHANGED(Follow_on_Chatter__c) , 
				ISCHANGED(Notify_on_First_Email_Only__c) , 
				ISCHANGED(Parent_Account__c) , 
				ISCHANGED(Product__c) , 
				ISCHANGED(User__c) 
		)
		  
	)
)</errorConditionFormula>
        <errorMessage>Can not populate any other fields if Case and Case CC Emails are populated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Notify_on_first_email_Only</fullName>
        <active>true</active>
        <description>Not allow User to select the &quot;Notify on first email only&quot; if either of the other two checkboxes are selected.</description>
        <errorConditionFormula>AND(OR(All_Customer_Communication__c , Follow_on_Chatter__c ) ,Notify_on_First_Email_Only__c )</errorConditionFormula>
        <errorDisplayField>Notify_on_First_Email_Only__c</errorDisplayField>
        <errorMessage>User is not allowed to select the &quot;Notify on first email only&quot; if either of the other two checkboxes are selected.</errorMessage>
    </validationRules>
</CustomObject>
