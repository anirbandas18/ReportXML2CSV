        <active>true</active>
        <errorConditionFormula>OR(AND(VLOOKUP($ObjectType.Reference_Key__c.Fields.Name, $ObjectType.Reference_Key__c.Fields.Name, Name) = Name, ISNEW()),AND(VLOOKUP($ObjectType.Reference_Key__c.Fields.Name, $ObjectType.Reference_Key__c.Fields.Name, Name) = Name, ISCHANGED(Name)))</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>The Reference Key already exists</errorMessage>
    </validationRules>
