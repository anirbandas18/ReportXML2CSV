    <deploymentStatus>Deployed</deploymentStatus>
    <description>The lookup table (LUT) will be created to define the different support level with each region (Support Center) and Parent Account (Customer name) and the Product Platform.</description>
    <enableActivities>false</enableActivities>
    <enableBulkApi>true</enableBulkApi>
    <enableFeeds>false</enableFeeds>
    <enableHistory>true</enableHistory>
    <enableReports>true</enableReports>
    <enableSearch>true</enableSearch>
    <enableSharing>true</enableSharing>
    <enableStreamingApi>true</enableStreamingApi>
    <externalSharingModel>Read</externalSharingModel>
    <fields>
        <fullName>Comments__c</fullName>
        <description>This field is a used to update the changes description/notes for Lookup table records.</description>
        <externalId>false</externalId>
        <label>Comments</label>
        <length>100000</length>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>LongTextArea</type>
        <visibleLines>4</visibleLines>
    </fields>
    <fields>
        <fullName>Lookup_Table_Formula__c</fullName>
        <description>This field is a used combine the Support Center, Parent Account and Product Platform field values.</description>
        <externalId>false</externalId>
        <formula>TEXT(Support_Center__c)+Parent_Account__c+Product_Platform__c</formula>
        <label>Lookup Table Formula</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Parent_Account__c</fullName>
        <deleteConstraint>Restrict</deleteConstraint>
        <description>This field is a lookup to Account to select the parent account records for Lookup table.</description>
        <externalId>false</externalId>
        <label>Parent Account</label>
        <lookupFilter>
            <active>true</active>
            <filterItems>
                <field>Account.Parent.Id</field>
                <operation>equals</operation>
                <value></value>
            </filterItems>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>Account</referenceTo>
        <relationshipLabel>Lookup Tables</relationshipLabel>
        <relationshipName>Lookup_Tables</relationshipName>
        <required>true</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Product_Platform__c</fullName>
        <deleteConstraint>Restrict</deleteConstraint>
        <description>This field is a lookup to Cadence Platform to select the parent account records for Lookup table</description>
        <externalId>false</externalId>
        <label>Product Platform</label>
        <lookupFilter>
            <active>true</active>
            <filterItems>
                <field>Cadence_Platform__c.Cos_Visibility__c</field>
                <operation>equals</operation>
                <value>True</value>
            </filterItems>
            <filterItems>
                <field>Cadence_Platform__c.Status__c</field>
                <operation>equals</operation>
                <value>ACTIVE</value>
            </filterItems>
            <filterItems>
                <field>Cadence_Platform__c.Name</field>
                <operation>notEqual</operation>
                <value>Design IP</value>
            </filterItems>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>Cadence_Platform__c</referenceTo>
        <relationshipLabel>Lookup Tables</relationshipLabel>
        <relationshipName>Lookup_Tables</relationshipName>
        <required>true</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
